{
  "parser": "babel-eslint",
  "extends": "./node_modules/eslint-config-google/index.js",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "max-len": 0,
    "eol-last": 0,
    "comma-dangle": [
      2,
      "never"
    ],
    "require-jsdoc": 0,
    "object-curly-spacing": 0,
    "brace-style": [
      2,
      "stroustrup"
    ],
    "space-before-function-paren": 0,
    "eqeqeq": [
      2,
      "always",
      {
        "null": "never"
      }
    ],
    "arrow-parens": [
      2,
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ],
    "new-cap": [
      2,
      {
        "capIsNewExceptions": [
          "DATE"
        ]
      }
    ]
  }
}